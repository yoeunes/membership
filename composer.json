{
    "name": "yoeunes/membership",
    "description": "Applicaton to create membership cards.",
    "keywords": ["membership", "yoeunes", "yoeunes", "khoubza", "association"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Younes Khoubza",
            "email": "younes.khoubza@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "alt-three/bus": "^3.0",
        "laravel-zero/framework": "^3.9.2",
        "maatwebsite/excel": "^2.1",
        "nunomaduro/collision": "^1.1",
        "simplesoftwareio/simple-qrcode": "^1.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["membership"]
}
